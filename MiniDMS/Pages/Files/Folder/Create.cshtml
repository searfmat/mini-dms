@page
@model MiniDMS.Pages.Files.Folder.CreateModel

@{
    ViewData["Title"] = "Create";
}




<h4>New Folder</h4>
<hr />
<div class="d-flex justify-content-center">

    <div >

        <form method="post" style="width: 350px;">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group w-full">
                    <label asp-for="FileModel.FileName" class="control-label"></label>
                    <input asp-for="FileModel.FileName" class="form-control" />
                    <span asp-validation-for="FileModel.FileName" class="text-danger"></span>
                
                   
                
                </div>
                <div class="mt-3">
                <input type="submit" value="Create" class="btn btn-primary float-end" />
                <a class="float-start btn btn-outline-secondary" asp-area="" asp-page="/Index">Back to List</a>
                </div>

            </form>
        </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
